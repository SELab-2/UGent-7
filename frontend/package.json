{
  "name": "vite-vue-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "host": "vite --host",
    "watch": "concurrently \"npm run host\" \"npm run reload\"",
    "reload": "onchange --await-write-finish 2000 '**/*.vue' -- echo > trigger.txt",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "concurrently": "^8.2.2",
    "onchange": "^7.1.0",
    "pinia": "^2.1.7",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.50.0",
    "vite-plugin-restart": "^0.4.0",
    "vue": "^3.4.18",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^5.0.4",
    "sass": "^1.72.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.1",
    "vue-tsc": "^1.8.27"
  }
}
